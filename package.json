{
  "name": "lumelines-daily-blocks",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode generic",
    "dev:generic": "vite --mode generic",
    "dev:crazygames": "vite --mode crazygames",
    "dev:poki": "vite --mode poki",
    "dev:yandex": "vite --mode yandex",
    "dev:vkplay": "vite --mode vkplay",
    "dev:rustore": "vite --mode rustore",
    "dev:newgrounds": "vite --mode newgrounds",
    "dev:itchio": "vite --mode itchio",
    "build": "npm run build:generic",
    "build:generic": "tsc -b && vite build --mode generic",
    "build:crazygames": "tsc -b && vite build --mode crazygames",
    "build:poki": "tsc -b && vite build --mode poki",
    "build:yandex": "tsc -b && vite build --mode yandex",
    "build:vkplay": "tsc -b && vite build --mode vkplay",
    "build:newgrounds": "tsc -b && vite build --mode newgrounds",
    "build:itchio": "tsc -b && vite build --mode itchio",
    "build:rustore": "tsc -b && vite build --mode rustore",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
